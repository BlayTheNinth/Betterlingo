{
	"name": "Duolingo but good",
	"version": "1.0",
	"description": "This is the Chrome Plugin.",
	"manifest_version": 2,
	"permissions": [
      "activeTab"
    ],
	"content_scripts": [
		{
			"matches": ["https://*.duolingo.com/*"],
			"css": ["duolingo.css"],
			"js": ["jquery-3.4.1.min.js", "wanakana.min.js", "duolingo.js"]
		}
	]
}